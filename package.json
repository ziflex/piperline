{
  "name": "piperline",
  "version": "0.3.3",
  "description": "Simple task pipeline runner.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint -c ./.eslintrc.json src",
    "test": "npm run lint && mocha --reporter spec --require babel-register --recursive test",
    "build": "npm run test && npm run clean && babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "keywords": [
    "async",
    "pipeline",
    "runner"
  ],
  "author": "Tim Voronov <ziflex@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "es6-symbol": "^2.0.1",
    "eventemitter3": "^1.1.1",
    "is-callable": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-mocha": "^2.0.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ziflex/piperline.git"
  }
}
